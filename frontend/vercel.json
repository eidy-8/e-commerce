{
    "version": 2,
    "builds": [
      {
        "src": "frontend/angular.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "frontend/dist/frontend" }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
  